AAA,IAAA,CAAA,CAAA,YAAA,CAAA,KAAA;AACA,UAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AAEA,UAAA,IAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,EAAA,IAAA;AACA,UAAA,OAAA,GAAA,IAAA,CAAA,WAAA,CAAA,GAAA,CAAA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,SAAA,CAAA,OAAA,GAAA,CAAA,CAAA;AAEA,WAAA,SAAA,GAAA,GAAA;AAEA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,UAAA,GAAA,EAAA,CAAA,CAAA,MAAA,CAAA;AADA,SAAA;AAGA,OAJA;;AAMA,MAAA,MAAA,CAAA,aAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAvGA;AAyGA,IAAA,IAAA,EAAA,gBAAA;AAAA;;AACA,UAAA,OAAA,GAAA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,IAAA,EAAA;AAAA,UAAA,KAAA,EAAA,GAAA;AAAA,UAAA,MAAA,EAAA;AAAA,SAFA;AAGA,QAAA,MAAA,EAAA,KAAA;AAHA,OAAA;AAMA,WAAA,KAAA,CAAA,UAAA,CAAA,MAAA,CAAA,OAAA,EAAA,UAAA,MAAA,EAAA;AACA,YAAA,MAAA,GAAA,MAAA,CAAA,SAAA,CAAA,MAAA,CAAA,WAAA,CAAA,GAAA,IAAA,CAAA,CAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,MAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,WAAA;AACA,OALA;AAMA,WAAA,SAAA,GAAA,KAAA;AACA,KAvHA;AAyHA,IAAA,QAAA,EAAA,oBAAA;AAAA;;AACA,WAAA,KAAA,CACA,GADA,WACA,KAAA,IADA,mBACA,KAAA,IAAA,CAAA,EADA,GAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,CAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA,CAAA;;AACA,QAAA,MAAA,CAAA,SAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,GAAA,EAAA,CAAA,CANA;AAOA;AAjIA,GArBA;AAwJA,EAAA,OAxJA,qBAwJA;AACA,SAAA,QAAA;AACA;AA1JA,CAAA","sourcesContent":["<template>\n  <div class=\"container d-flex justify-content-center\">\n    <div style=\"width: 350px;\" class=\"mb-4 position-relative\">\n      <div\n        v-if=\"loading\"\n        style=\"height: 400px;\"\n        class=\"d-flex justify-content-center align-items-center\"\n      >\n        <b-spinner variant=\"primary\" label=\"Spinning\"></b-spinner>\n      </div>\n      <div v-else>\n        <div v-if=\"imageSrc.length > 0\" class=\"d-flex justify-content-center\">\n          <hooper style=\"height: 400px; width: 300px\" class=\"rounded-lg shadow\">\n            <slide\n              v-for=\"(image, index) in imageSrc\"\n              :key=\"index\"\n              class=\"rounded-lg\"\n            >\n              <img :src=\"image.image64\" style=\"height: 400px; width: 300px,object-fit: cover, left: 50%;\n  position: relative;\" class=\"rounded-lg\" />\n            </slide>\n            <hooper-navigation slot=\"hooper-addons\"></hooper-navigation>\n          </hooper>\n        </div>\n        <div v-else class=\"d-flex justify-content-center\">\n          <hooper style=\"height: 400px; width: 300px\" class=\"rounded-lg shadow\">\n            <slide class=\"rounded-lg\">\n              <img src=\"@/assets/no-photo.png\" style=\"height: 400px; width: 300px, object-fit: cover\" class=\"rounded-lg\" />\n            </slide>\n          </hooper>\n        </div>\n      </div>\n      <div v-if=\"myprofile\">\n        <b-button\n          style=\"position: absolute; right: -5px; bottom: -10px; z-index: 1000\"\n          pill\n          class=\"bg-secondary display-1 shadow\"\n          size=\"lg\"\n          @click=\"showModal('uploads')\"\n        >\n          <b-icon variant=\"light\" class=\"mb-1 mt-2\" icon=\"pencil\"></b-icon>\n        </b-button>\n        <b-modal\n          ref=\"uploads\"\n          hide-footer\n          title=\"Upload or Change Photos\"\n          class=\"text-center\"\n        >\n          <div v-if=\"uploading\">\n            <vue-croppie\n              ref=\"croppieRef\"\n              :boundary=\"{ width: 300, height: 400 }\"\n              :viewport=\"{ width: 300, height: 400, type: 'square' }\"\n              :showZoomer=\"false\"\n              :enableResize=\"false\"\n              class=\"position-relative\"\n            >\n            </vue-croppie>\n            <b-button\n              style=\"font-size: 2rem; border: none; position: absolute; right: 80px; bottom: 15px; z-index: 1000\"\n              variant=\"primary\"\n              class=\"rounded-pill lg shadow-lg pt-2\"\n              @click=\"crop\"\n            >\n              <b-icon icon=\"check2-circle\"></b-icon>\n            </b-button>\n          </div>\n          <div v-else>\n            <div\n              v-if=\"imageSrc.length > 0\"\n              class=\"d-flex justify-content-center flex-wrap mx-md-5\"\n            >\n              <div\n                v-for=\"(image, index) in imageSrc\"\n                :key=\"index\"\n                class=\"mx-2 mb-3 mt-2 rounded-lg\"\n                style=\"width: 80px\"\n              >\n                <div class=\"position-relative\"\n                style=\"font-size: 1.5rem\">\n                  <img\n                    :src=\"image.image64\"\n                    style=\"width: 80px\"\n                    class=\"rounded-lg\"\n                  />\n                  <b-icon\n                    style=\"border: none; position: absolute; right: 5px; bottom: 5px; z-index: 1000\"\n                    variant=\"danger\"\n                    class=\"rounded-pill lg shadow-lg bg-light\"\n                    @click=\"removeImage(image.id)\"\n                    icon=\"x-circle\"\n                  ></b-icon>\n                </div>\n              </div>\n              <div\n                v-if=\"imageSrc.length < 5\"\n                class=\"mx-2 mb-3 mt-2 d-flex align-items-center justify-content-center border border-primary rounded-lg bg-light\"\n                style=\"width: 80px\"\n              >\n                <label for=\"img\" style=\"font-size: 3rem\" class=\"\"\n                  ><b-icon variant=\"primary\" icon=\"plus-circle\"></b-icon\n                  ><span style=\"font-size: 1rem\"></span\n                ></label>\n              </div>\n              <div v-else>\n                <div\n                  class=\"mx-2 mb-3 mt-2 d-flex align-items-center justify-content-center text-primary\"\n                  style=\"width: 80px, height: auto\"\n                >\n                  <span class=\"small\">Maximum pictures reached.</span>\n                </div>\n              </div>\n            </div>\n            <div\n              v-else\n              class=\"mx-2 mb-3 mt-2 d-flex align-items-center justify-content-center border border-primary rounded-lg bg-light\"\n              style=\"width: 80px\"\n            >\n              <label for=\"img\" style=\"font-size: 3rem\" class=\"\"\n                ><b-icon variant=\"primary\" icon=\"plus-circle\"></b-icon\n                ><span style=\"font-size: 1rem\"></span\n              ></label>\n            </div>\n            <input\n              type=\"file\"\n              @change=\"croppie\"\n              name=\"uploadfile\"\n              id=\"img\"\n              style=\"display:none;\"\n              accept=\".jpg, .png\"\n            />\n          </div>\n        </b-modal>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Hooper, Slide, Navigation as HooperNavigation } from \"hooper\";\nimport \"hooper/dist/hooper.css\";\n\nimport {actions, state} from \"@/store\"\n\nexport default {\n  props: [\"user\", \"myprofile\"],\n  components: {\n    Hooper,\n    Slide,\n    HooperNavigation,\n  },\n  data() {\n    return {\n      images64: [],\n      imageSrc: [],\n      loading: true,\n      upload: {\n        id: null,\n        is_primary: true,\n        image_type: \"\",\n        image64: \"\",\n      },\n      uploading: false,\n    };\n  },\n  methods: {\n    getBase64: function() {\n      if (this.images64) {\n        this.imageSrc = this.images64.filter(function(image) {\n          return image.image64 != '&nbsp'\n        });\n      }\n\n      if (this.imageSrc) {\n        this.imageSrc.forEach((image) => {\n          image.image64 =\n            \"data:image/\" + image.image_type + \";base64, \" + image.image64;\n        });\n      }\n      this.loading = false;\n    },\n\n    showModal(ref) {\n      this.$refs[ref].show();\n    },\n\n    uploadImage: function() {\n      if (this.images64) {\n        this.images64.forEach((image, array, index) => {\n          if (image.is_primary) {\n            this.upload.is_primary = false;\n          }\n          if (image.image64 === \"&nbsp\") {\n            this.upload.id = image.id;\n            this.upload.is_primary = image.is_primary;\n          }\n        });\n      }\n\n      this.$http\n        .put(`${this.$api}/images`, this.upload)\n        .then((res) => {\n          this.imageSrc = [];\n          this.loadUser();\n          let test = this.imageSrc;\n          this.$bvToast.toast(\"Image uploaded.\", {\n            title: \"Success!\",\n            autoHideDelay: 2000,\n            toaster: \"b-toaster-top-center\",\n            variant: \"info\",\n            noCloseButton: true,\n          });\n        })\n        .catch((err) => {\n          this.$bvToast.toast(err.response.data.message, {\n            title: \"Error!\",\n            autoHideDelay: 5000,\n            variant: \"danger\",\n          });\n        });\n    },\n\n    removeImage: function(id) {\n      this.upload.id = id;\n      this.upload.image64 = \"&nbsp\";\n      this.upload.image_type = \"&nbsp\";\n\n      this.$http\n        .put(`${this.$api}/images`, this.upload)\n        .then((res) => {\n          this.imageSrc = [];\n          this.loadUser();\n          this.$bvToast.toast(\"Image deleted.\", {\n            title: \"Success!\",\n            autoHideDelay: 2000,\n            toaster: \"b-toaster-top-center\",\n            variant: \"info\",\n            noCloseButton: true,\n          });\n        })\n        .catch((err) => {\n          this.$bvToast.toast(err.response.data.message, {\n            title: \"Error!\",\n            autoHideDelay: 5000,\n            variant: \"danger\",\n          });\n        });\n    },\n\n    croppie: function(e) {\n      this.uploading = true;\n      var files = e.target.files || e.dataTransfer.files;\n      if (!files.length) return;\n\n      const name = e.target.files[0].name;\n      const lastDot = name.lastIndexOf(\".\");\n      const ext = name.substring(lastDot + 1);\n\n      this.extension = ext;\n\n      var reader = new FileReader();\n      reader.onload = (e) => {\n        this.$refs.croppieRef.bind({\n          url: e.target.result,\n        });\n      };\n\n      reader.readAsDataURL(files[0]);\n    },\n\n    crop: function() {\n      let options = {\n        type: \"base64\",\n        size: { width: 300, height: 400 },\n        format: this.extension,\n      };\n\n      this.$refs.croppieRef.result(options, (output) => {\n        const base64 = output.substring(output.lastIndexOf(\",\") + 1);\n        this.upload.image64 = base64;\n        this.upload.image_type = this.extension;\n        this.uploadImage();\n      });\n      this.uploading = false;\n    },\n\n    loadUser: function() {\n      this.$http\n        .get(`${this.$api}/user/${this.user.id}`)\n        .then((res) => {\n          this.images64 = _.cloneDeep(res.data.images);\n          this.getBase64();\n        })\n        .catch((err) => {});\n    },\n  },\n  mounted() {\n    this.loadUser();\n  },\n};\n</script>\n"],"sourceRoot":"src/components/profile-components"}]}